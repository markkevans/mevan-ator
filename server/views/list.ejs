<h2>User Requests</h2>

<table border="1">
<thead>
  <tr>
    <th>User ID</th>
    <th>Time</th>
    <th>Expected Responses</th>
  </tr>
 </thead>
 <tbody>
 <% requests.forEach( function(item) { %>
  <tr>
    <td><a href="/responses/<%=item.user_id%>"><%= item.user_id %></a></td>
    <td><%= item.date_time %></td>
    <td>Coming Soon</td>
  </tr>
 <% }); %>
</tbody>
</table>